{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "moduleDetection": "force",
    "module": "NodeNext",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noPropertyAccessFromIndexSignature": true,
    "allowImportingTsExtensions": true,
    "preserveSymlinks": true
  }
}
